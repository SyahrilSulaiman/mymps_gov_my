ak.\"}),Object(s.jsxs)(\"div\",{className:\"sm:block flex flex-col mt-10\",children:[Object(s.jsx)(\"a\",{href:\"/login\",target:\"_blank\",className:\"get-started text-white font-bold px-6 py-4 rounded outline-none focus:outline-none mr-1 mb-2 bg-blue-500 active:bg-blue-600 uppercase text-sm shadow hover:shadow-lg ease-linear transition-all duration-150\",children:\"Login\"}),Object(s.jsx)(\"a\",{href:\"/register\",target:\"_blank\",className:\"github-star sm:ml-1 text-white font-bold px-6 py-4 rounded outline-none focus:outline-none mr-1 mb-1 bg-gray-800 active:bg-gray-700 uppercase text-sm shadow hover:shadow-lg\",children:Object(s.jsx)(\"span\",{children:\"Daftar Pengguna\"})})]}),Object(s.jsx)(\"div\",{className:\"text-center mt-16\"})]})})})]}),Object(s.jsx)(M,{})]})}var Q=function(){return Object(s.jsx)(\"div\",{children:Object(s.jsx)($,{})})};t(87);var X=function(e){var a=Object(l.useState)(e),t=Object(d.a)(a,2),s=t[0],r=t[1];return{value:s,onChange:function(e){r(e.target.value)}}},ee=function(e){sessionStorage.removeItem(\"GoogleToken\"),sessionStorage.removeItem(\"GoogleEmail\"),sessionStorage.removeItem(\"GoogleName\");var a=X(\"\"),t=X(\"\"),n=X(\"\"),c=X(\"\"),o=X(\"\"),m=X(\"\"),b=X(\"\"),x=X(\"\"),j=/^[0-9]+$/,u=Object(l.useState)(null),p=Object(d.a)(u,2),h=(p[0],p[1]),f=Object(l.useState)(!1),g=Object(d.a)(f,2),O=(g[0],g[1]),w=r.a.useState(1),y=Object(d.a)(w,2),v=y[0],k=y[1];return Object(s.jsxs)(\"div\",{className:\"bg-gray\",children:[Object(s.jsx)(P,{fixed:!0}),Object(s.jsx)(\"section\",{className:\"py-20 relative\",style:Object(N.a)({background:\"rgb(34,81,122)\"},\"background\",\"linear-gradient(90deg, rgba(34,81,122,1) 0%, rgba(27,147,171,1) 100%)\"),children:Object(s.jsx)(\"div\",{class:\"bg-grey-lighter min-h-screen flex flex-col\",children:Object(s.jsx)(\"div\",{class:\"container max-w-sm mx-auto flex-1 flex flex-col items-center justify-center px-2\",children:Object(s.jsxs)(\"div\",{class:\"bg-white px-6 py-8 rounded shadow-md text-black w-